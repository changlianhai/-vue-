	<div class="hl-withdraw-total-withdraw-settings ">
		<div class="btn btn-primary hl-withdraw-total-withdraw-settings-btn" onclick="catdv.withdraw_start()">提取</div>

	<div class="w-pad wd-btn-box">
		<button class="btn btn-primary btn-block" onclick="withdraw_start()" type="button">提取</button>

	</div>
</section>


