<!-- 挖矿-介绍挖币（停止挖矿） -->
<section class="wrap bg-wrap bg-muted hl-mn-more">
	<ul class="hl-mn-list-group hl-mn-more-list mar-t1 clearfix">
		<li onclick="catdv.miner_status()">
			<a>
				<label>矿机管理</label>
				<div class="fr">
					<span>{{mining.minerStatus.length}}台</span>
					<i class="arrow-r"></i>
				</div>
			</a>
		</li>
		<li onclick="catdv.address_settings()">
			<a>
				<label>收款地址</label>
