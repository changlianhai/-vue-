					<span class="address">{{mining.data.address}}</span>
					<i class="arrow-r"></i>
				</div>
			</a>
		</li>
		<li onclick="catdv.withdraw_record()">
			<a>
				<label>提币记录</label>
				<div class="fr">
					<span></span>
					<i class="arrow-r"></i>
				</div>
			</a>
		</li>
		<li>
			<a>
				<label>挖矿攻略</label>
				<div class="fr">
					<span></span>
					<i class="arrow-r"></i>
				</div>
			</a>
		</li>
	</ul>
</section>