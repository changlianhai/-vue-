/**
 * catdv_set_title: 设置页面的title
 * @param {{string}} t: 页面中文名
 * @return  void;
 */
function catdv_set_page_title (modelName){
    var p=hl_config.page[modelName];
    var tit=p.tit;
    catdv.datas.apd.nav.title=tit;
}

