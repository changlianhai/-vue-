/*  收货地址模块样式   */
#hl_router, #temp_add, #add {
    height: 100%;
    overflow: hidden;
}

.withdraw-address-model {
    padding: 1rem 0;
    background-color: #f0f0f0;
    height: 100%;
}

.withdraw-address {
    padding: 1rem;
    width: 100%;
    background-color: #fff;
    border: 1px solid #ccc;
    font-size: 1.2rem;
    border-left: none;
    border-right: none;
    letter-spacing: 2px;

}

.withdraw-get-address {
    padding: 1rem;
    font-size: 1.2rem;
    color: #00AFEC;
}

/*  提币模块样式   */
#total {
    height: 100%;
    background-color: #F4F4F4;
}
.total-withdraw-model {
    height: 100%;
    background-color: #F4F4F4;
}
.total-info {
    padding: 1rem;
    font-size: 1.2rem;
    border-bottom: 1rem solid #F4F4F4;
    background-color: #fff;
}

.total-info-address {
    padding: .5rem .5rem;
    background-color: #F7F7F7;
}

.total-info-title {
    padding: .5rem .5rem;
}

.total-info b {
    font-weight: normal;
}

.total-input-sum {
    display: flex;
    display: -webkit-flex;
    padding: 1rem;
    background-color: #F7F7F7;

}

.total-input-sum-info {
    padding-left: 2rem;
    flex-grow: 1;
    -webkit-flex-grow: 1;

}
.total-input-sum-all {
    text-align: right;
    width:6rem;
    color: #00AFEC;
    font-size: 1.2rem;
}

.total-info-price {
    padding-left:.5rem;
}
.total-info-price span {
    font-size: 1.1rem;
}

.total-withdraw-settings {
    padding: 4rem 1rem;

}
.total-withdraw-settings-btn {
    padding:0;
    display: block;
    font-size:1.2rem;
}




/*  开通成功页面样式 */

.open-success-model-tips {
    padding: 6rem 0;
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;

}

.open-success-model-tips-time {
    padding: 2rem 0;
    font-size: 1.3rem;
    color: #666;
}

.open-success-model-cont {
    padding: 0 1.5rem;
}

.open-success-model-li {
    padding:1rem 0;
    display: flex;
    display: -webkit-flex;
    justify-content: center;
    align-items: center;
    border-bottom:1px solid #ccc;
}

.open-success-model-li-key {
    -webkit-flex-basis: 5rem;
    flex-basis: 6rem;
    font-size: 1.2rem;
    color:#999;

}

.open-success-model-li-val {
    -webkit-flex-grow: 1;
    flex-grow: 1;
    text-align: right;
    font-size: 1.2rem;
    color:#333;

}
.open-success-model-settings {
    padding: 4rem 1rem;
}
.open-success-model-settings-btn {
    padding:0;
    display: block;
    font-size:1.2rem;
}


/*  提币记录模块样式   */
.record-model {
    padding:1rem 1.5rem;

}
.record-model-list {
    border-bottom:1px solid #ccc;
}
.record-model-list-info {
    padding:1rem 0;
    display: table;
    width:100%;
    vertical-align: middle;
}
.record-model-list-info-tips {
    display: table-cell;
    width:10rem;
    font-size: 1.2rem;
}
.record-model-list-info-number {
    display: table-cell;
    vertical-align: middle;
    text-align: right;
    color: #00AFEC;
    font-size: 1.2rem;
}
.record-model-list-info-order {
    padding:0  0 1rem 0;
    font-size: 1.2rem;
}
.record-model-list-info-address {
    padding-top:.5rem;
    color:#333;
    font-size: 1.2rem;
}