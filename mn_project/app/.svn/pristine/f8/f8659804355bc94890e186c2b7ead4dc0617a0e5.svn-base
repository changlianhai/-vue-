.btn-box{position:relative; overflow:hidden; margin: 0 15px;}
.btn-box span{position:absolute; left:0; width:100%; height:100%;}
.btn-blue{ background: #00a0e9; color:#f2f2f3; box-shadow:0 0 0 2px #00a0e9 inset;}
.btn-font{position:absolute; left:0; z-index:10; width:100%;transition:all 0.2s ease 0s;}
.btn-box:hover .btn-font{color:#000;}
.btn-box .btn-default{-webkit-transition: -webkit-transform 0.2s; -moz-transition: -moz-transform 0.2s; -ms-transition: -ms-transform 0.2s; transition: transform 0.2s; -webkit-transform: translate3d(0,0,0); -moz-transform: translate3d(0,0,0); -ms-transform: translate3d(0,0,0); transform: translate3d(0,0,0);}	
.btn-box .btn-hover{-webkit-transition: -webkit-transform 0.2s; -moz-transition: -moz-transform 0.2s; -ms-transition: -ms-transform 0.2s; transition: transform 0.2s; -webkit-transform: translate3d(0,100%,0); -moz-transform: translate3d(0,100%,0); -ms-transform: translate3d(0,100%,0); transform: translate3d(0,100%,0); color:#f2f2f3; border-radius:24px; background-color:#f2f2f3; text-decoration: none;}
.btn-box:hover .btn-default{-webkit-transform: translate3d(0,-100%,0); -moz-transform: translate3d(0,-100%,0); -ms-transform: translate3d(0,-100%,0);	transform: translate3d(0,-100%,0);}	
.btn-box:hover .btn-hover{-webkit-transform: translate3d(0,0,0); -moz-transform: translate3d(0,0,0); -ms-transform: translate3d(0,0,0);	transform: translate3d(0,0,0);}
